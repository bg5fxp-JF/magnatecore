{
	"version": 2,
	"builds": [
		{ "src": "/home/package.json", "use": "@vercel/next" },
		{ "src": "/denis/package.json", "use": "@vercel/node" },
		{ "src": "/jacob/package.json", "use": "@vercel/next" }
	],
	"routes": [
		{ "src": "^/home/(.*)", "dest": "/home/$1" },
		{ "src": "^/denis/(.*)", "dest": "/denis/$1" },
		{ "src": "^/jacob/(.*)", "dest": "/jacob/$1" }
	]
}
